{
  "consumer": {
    "name": "web-api"
  },
  "interactions": [
    {
      "description": "a query for the price",
      "providerState": "There is a certification price",
      "request": {
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "GET",
        "path": "/certifications/price"
      },
      "response": {
        "body": {
          "price": 100
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body.price": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a certification request",
      "providerState": "There is a certifications endpoint",
      "request": {
        "body": {
          "name": "someone",
          "userId": "ea4ceefe-cfe6-49e8-a36d-1a889c780bb4"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.name": {
            "match": "type"
          },
          "$.body.userId": {
            "match": "type"
          }
        },
        "method": "POST",
        "path": "/certifications"
      },
      "response": {
        "body": {
          "certificateId": "string",
          "certifiedName": "string",
          "dateCertified": "string"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body.certificateId": {
            "match": "type"
          },
          "$.body.certifiedName": {
            "match": "type"
          },
          "$.body.dateCertified": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.6.4"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "certifier"
  }
}
